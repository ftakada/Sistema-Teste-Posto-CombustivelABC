unit Sistema.Model.Entities.Bombas;

interface


type
  TTanques = class
  private
    FID: Integer;
    FDESCRICAO: string;
    FID_TANQUE : Integer;
  public
    property ID: Integer read FID write FID;
    property DESCRICAO: string read FDESCRICAO write FDESCRICAO;
    property ID_TANQUE: Integer read FID_TANQUE write FID_TANQUE;
  end;

implementation

end.
