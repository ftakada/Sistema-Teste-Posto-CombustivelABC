unit Sistema.Model.Entities.Tanques;

interface


type
  TTanques = class
  private
    FID: Integer;
    FDESCRICAO: string;
    FTIPOCOMBUSTIVEL : Integer;
  public
    property ID: Integer read FID write FID;
    property DESCRICAO: string read FDESCRICAO write FDESCRICAO;
    property TIPOCOMBUSTIVEL: Integer read FTIPOCOMBUSTIVEL write FTIPOCOMBUSTIVEL;
  end;

implementation

end.
