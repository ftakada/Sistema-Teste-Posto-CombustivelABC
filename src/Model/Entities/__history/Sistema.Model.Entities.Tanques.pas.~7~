unit Sistema.Model.Entities.Tanques;

interface


type
  TTanques = class
  private
    FID: Integer;
    FDescricao: string;
    FTipoCombustivel : Integer;
  public
    property ID: Integer read FID write FID;
    property Nome: string read FNome write FNome;
    property TipoCombustivel: Integer read TipoCombustivel write TipoCombustivel;
  end;

implementation

end.
